import java.util.*;
public class main
{
  public static  void main (string [] args)
  {
    string str="temple";
    srting rev=" ";
    for(int i=str.length()-1;i>=0;i--)
      {
      rev+=str.charAt(i);
      }
    system.out.println("string:"+str);
    system.out.println("reverse:"+rev);
    }
  }
    
